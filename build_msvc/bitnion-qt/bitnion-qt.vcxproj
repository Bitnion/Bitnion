    <ClCompile Include="..\..\src\qt\main.cpp" />
    <ClCompile Include="..\..\src\chainparams.cpp" />
    <ClCompile Include="..\..\src\validation.cpp" />
    <ClCompile Include="..\..\src\pow.cpp" />
  </ItemGroup>

