        $(QTDIR)\include\QtWidgets;
        $(QTDIR)\include\QtCore;
        $(QTDIR)\include\QtGui
      </AdditionalIncludeDirectories>
      <PreprocessorDefinitions>WIN32;NDEBUG;_WINDOWS;QT_WIDGETS_LIB;HAVE_CONFIG_H</PreprocessorDefinitions>
      <RuntimeLibrary>MultiThreaded</RuntimeLibrary>
    </ClCompile>
    <Link>
      <AdditionalDependencies>
        Qt5Widgets.lib;
        Qt5Core.lib;
        Qt5Gui.lib;
        libbitnion_util.lib;
        libbitnion_wallet.lib;
        libbitnion_common.lib;
        libbitnion_server.lib;
        ws2_32.lib;
        shell32.lib;
        user32.lib;
        gdi32.lib
      </AdditionalDependencies>
      <SubSystem>Windows</SubSystem>
    </Link>
  </ItemDefinitionGroup>

  <ItemGroup>
    <ClCompile Include="..\..\src\qt\bitnion.cpp" />
