<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup Label="ProjectConfigurations">
    <ProjectConfiguration Include="Release|x64">
      <Configuration>Release</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
  </ItemGroup>

  <PropertyGroup Label="Globals">
    <ProjectGuid>{B1C7-BITNIOND-DAEMON-GUID}</ProjectGuid>
    <Keyword>Win32Proj</Keyword>
    <RootNamespace>bitniond</RootNamespace>
    <ProjectName>bitniond</ProjectName>
  </PropertyGroup>

  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.Default.props" />

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'" Label="Configuration">
    <ConfigurationType>Application</ConfigurationType>
    <UseDebugLibraries>false</UseDebugLibraries>
    <PlatformToolset>v143</PlatformToolset>
    <CharacterSet>Unicode</CharacterSet>
  </PropertyGroup>

  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.props" />

  <ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <ClCompile>
      <AdditionalIncludeDirectories>..\..\src;..\..\src\config;..\..\src\interfaces;..\..\src\wallet</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>WIN32;NDEBUG;_CONSOLE;HAVE_CONFIG_H</PreprocessorDefinitions>
      <RuntimeLibrary>MultiThreaded</RuntimeLibrary>
    </ClCompile>
    <Link>
      <AdditionalDependencies>
        libbitnion_common.lib;
        libbitnion_util.lib;
        libbitnion_wallet.lib;
        libbitnion_server.lib;
        ws2_32.lib;
        shlwapi.lib
      </AdditionalDependencies>
      <SubSystem>Console</SubSystem>
    </Link>
  </ItemDefinitionGroup>

  <ItemGroup>
    <ClCompile Include="..\..\src\bitniond.cpp" />
    <ClCompile Include="..\..\src\chainparams.cpp" />
    <ClCompile Include="..\..\src\validation.cpp" />
    <ClCompile Include="..\..\src\pow.cpp" />
  </ItemGroup>

  <ItemGroup>
    <ClInclude Include="..\..\src\chainparams.h" />
    <ClInclude Include="..\..\src\validation.h" />
    <ClInclude Include="..\..\src\pow.h" />
    <ClInclude Include="..\..\src\config\bitnion-config.h" />
  </ItemGroup>

  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />
</Project>
