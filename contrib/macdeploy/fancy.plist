<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" \
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>background</key>
  <string>.background/background.png</string>

  <key>icon-size</key>
  <integer>96</integer>

  <key>window-size</key>
  <array>
    <integer>600</integer>
    <integer>400</integer>
  </array>

  <key>icon-position</key>
  <dict>
    <key>Bitnion-Qt.app</key>
    <array>
      <integer>120</integer>
      <integer>150</integer>
    </array>
    <key>Applications</key>
    <array>
      <integer>380</integer>
      <integer>150</integer>
    </array>
  </dict>

  <key>hide-extension</key>
  <array>
    <string>Bitnion-Qt.app</string>
  </array>

  <key>show-status-bar</key>
  <false/>

  <key>show-tab-view</key>
  <false/>

  <key>show-toolbar</key>
  <false/>

  <key>arrange-by</key>
  <string>none</string>
</dict>
</plist>
